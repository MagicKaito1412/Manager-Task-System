{{#if hasMoreItems}}
<div class="x-list-paginator">
    {{#if message}}
        <div class="text-info"><span class="x-icon x-icon-info"></span>{{message}}</div>
    {{/if}}
    {{#if-menu menu}}
        {{#if-not options.hidePageStat}}
            <span class="x-list-paginator_stat">{{pageStat}}</span>
        {{/if-not}}
        <div id="{{push-id}}" class="x-menu-bar">
            {{part (object cssClass="center-block -inline-block") class="ui.MenuPresenter" viewModel=menu target=":pop-id"}}
			<span style="vertical-align: top; display: inline-block;">
				<input class="form-control x-list-paginator_goto-input" type="number" {{data-bind value="currentPage"}} min="1" max="{{lastPage}}" />
			</span>
        </div>
    {{/if-menu}}
</div>
{{/if}}
